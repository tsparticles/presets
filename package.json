{
  "name": "@tsparticles/presets-workspace",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "build:docs": "rimraf docs && typedoc --plugin typedoc-plugin-keywords --plugin typedoc-plugin-coverage",
    "version:alpha": "lerna version prerelease --preid alpha --conventional-commits",
    "version:beta": "lerna version prerelease --preid beta --conventional-commits",
    "publish:alpha": "lerna publish from-package --pre-dist-tag alpha --preid alpha",
    "publish:beta": "lerna publish from-package --pre-dist-tag beta --preid beta",
    "publish:next": "lerna publish from-package --dist-tag next"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@tsparticles/cli": "^3.0.14",
    "@tsparticles/eslint-config": "^3.0.9",
    "@tsparticles/prettier-config": "^3.0.7",
    "@tsparticles/tsconfig": "^3.0.9",
    "@tsparticles/webpack-plugin": "^3.0.9",
    "@types/node": "^25.0.3",
    "@types/webpack-env": "^1.18.8",
    "babel-loader": "^10.0.0",
    "browserslist": "^4.28.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "fs-extra": "^11.3.3",
    "husky": "^9.1.7",
    "lerna": "^9.0.3",
    "prettier": "^3.7.4",
    "prettier-plugin-multiline-arrays": "^4.1.3",
    "rimraf": "^6.1.2",
    "terser-webpack-plugin": "^5.3.16",
    "typedoc": "^0.28.15",
    "typedoc-plugin-coverage": "^4.0.2",
    "typedoc-plugin-keywords": "^1.6.0",
    "typedoc-plugin-missing-exports": "^4.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^5.1.1",
    "webpack-cli": "^6.0.1"
  },
  "workspaces": [
    "apps/*",
    "presets/*"
  ],
  "type": "module"
}
